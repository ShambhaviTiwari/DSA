class Solution:
    def rearrangeArray(self, nums: List[int]) -> List[int]:
        ans = []
        pos = 0
        neg = 1
        for i in range(0,len(nums)):
            if nums[i]>0:
                ans.insert(pos,nums[i])
                pos +=2
            else:
                ans.insert(neg,nums[i])
                neg += 2
        return ans







        
        
        
        
        
    
        
